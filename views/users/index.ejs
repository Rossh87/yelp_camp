<% include ../partials/header %>

<div class="container">
	<div class="list-group">
		<% allUsers.forEach(function(user){%>
			<a href="/users/<%= user._id %>" class="list-group-item">
				<h4 class="list-group-item-heading"><%= user.username %></h4>
				<p class="list-group-item-text"><em>Member since <%= moment(user.userName).fromNow()%></em></p>
			</a>

		<% }) %>
	</div>
</div>










<% include ../partials/footer %>